}else{
crs <- crs
}
return_sf <- sf::st_read(read_path) %>%
sf::st_transform(crs = crs)
}else{
return_sf <- sf::st_read(read_path)
}
# Do you want a simplified map?
if(simple != "NA"){
return_sf <- return_sf %>%
sf::st_simplify(preserveTopology = TRUE, dTolerance = simple)
}
# If no options are selected
# return_sf <- sf::st_read(read_path)
return(return_sf)
}
